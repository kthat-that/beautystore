  <body>
    <!-- Top Menu -->
    <section>
      <div class="container-fluid menu">
        <div class="container">
          <div class="row">
            <nav class="col-12">
              <div class="logo">
                
                <img src="/img/imgs/logo.png" alt="logo" />
              </div>
              <ul>
                <li><a href="/Homepage">Home</a></li>
                <li><a href="/Skincare">Skincare</a></li>
                <li><a href="/About">About</a></li>
                <li><a href="/Contact">Contact</a></li>
