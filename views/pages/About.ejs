                <i class="fa-regular fa-user"></i>
                <i class="fa-solid fa-cart-shopping"></i>
              </div>

               <div class="d-flex justify-content-between">
                  <% if (!user) { %>
                    <div class="me-4">
                      <a href="/signin">
                        <button class="btn btn-primary btn_auth">Login</button>
                      </a>
                    </div>
                    <div>
                      <a href="/register">
                        <button class="btn btn-outline-primary btn_auth">Sign Up</button>
                      </a>
                    </div>
                  <% } else { %>
                    <div>
                      <a href="/logout">
                        <button class="btn btn-danger">Logout</button>
                      </a>
                    </div>
                  <% } %>
              </div>
            </nav>
          </div>
        </div>
      </div>
